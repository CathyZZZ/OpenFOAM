FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    axis
    {
        type            empty;
    }

    inlet
    {
        type            fixedValue;
        value           uniform 1000000;
    }

    wall
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
   front
    {
        type            wedge;
    }
   back
    {
        type            wedge;
    }

}

