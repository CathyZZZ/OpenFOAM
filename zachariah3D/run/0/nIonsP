FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nIonsP;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 -3 0 0 1 0 0 ];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    lowerWall0
    {
        type            fixedValue;
        value           uniform 0;
    }
    lowerWall1
    {
        type            fixedValue;
        value           uniform 0;
    }
    lowerWall2
    {
        type            fixedValue;
        value           uniform 0;
    }
    upperWall0
    {
        type            fixedValue;
        value           uniform 0;
    }
    upperWall1
    {
        type            fixedValue;
        value           uniform 0;
    }
    upperWall2
    {
        type            fixedValue;
        value           uniform 0;
    }
    frontAndBack_half0
    {
        type            cyclic;
    }
    frontAndBack_half1
    {
        type            cyclic;
    }
}


